<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:test='urn:import:com.googlecode.gwt.test.uibinder'>
	<g:Grid>
		<g:row styleName="optionalHeaderStyle">
			<g:customCell styleName="optionalFooCellStyle">
				<g:Label ui:field="myLabel">my label</g:Label>
			</g:customCell>
			<g:customCell>
				<g:HTML ui:field="myHTML">my html</g:HTML>
			</g:customCell>
		</g:row>
		<g:row>
			<g:cell>
				<div ui:field="myDiv">foo</div>
			</g:cell>
			<g:cell styleName="optionalSpanCellStyle">
				<span ui:field="mySpan">bar</span>
			</g:cell>
		</g:row>
	</g:Grid>
</ui:UiBinder>